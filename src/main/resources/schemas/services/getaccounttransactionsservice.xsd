<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.mybankapp.org/getaccounttransactions/wsdl"
           targetNamespace="http://www.mybankapp.org/getaccounttransactions/wsdl" elementFormDefault="qualified">

    <xs:element name="getAccountTransactionsRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="accountId" type="xs:long"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getAccountTransactionsResponse" >
        <xs:complexType>
            <xs:sequence>
                <xs:element name="transactionDTO" minOccurs="0" maxOccurs="unbounded" type="tns:transactionDTO"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="transactionDTO">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="date" type="xs:dateTime"/>
            <xs:element name="amount" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>


</xs:schema>